.page {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.page .navigation {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  padding-top: 40rpx;
  line-height: 88rpx;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 128rpx;
  background: #f2f2f2;
  border-bottom: 1px solid #dadada;
}

.page .navigation .back-bn {
  position: absolute;
  left: 20rpx;
  bottom: 0rpx;
  height: 77rpx;
  padding: 0 10rpx;
  vertical-align: middle;
}

.page .navigation .back-bn image {
  width: 20rpx;
  height: 32rpx;
}

.page .navigation text {
  font-size: 36rpx;
  font-weight: bold;
}

.page .msg-box .sticks {
  width: 90rpx;
  margin-top: 37rpx;
}

@-webkit-keyframes ring {
  0% {
    -webkit-transform: rotate(0);
    transform: rotate(0);
  }
  50% {
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}

@keyframes ring {
  0% {
    -webkit-transform: rotate(0);
    transform: rotate(0);
  }
  50% {
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}

.page .msg-box .sticks .rotate {
  -webkit-animation: 2s linear 0s normal none infinite ring;
  animation: 2s linear 0s normal none infinite ring;
}

.page .msg-box .sticks .line {
  width: 6rpx;
  margin: 0 auto;
}

.page .msg-box .sticks .line.short_line {
  height: 24rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100%;
}

.page .msg-box .sticks .line.long_line {
  height: 110rpx;
  background: url(data:image/png;base64,) no-repeat center center;
  background-size: 100%;
}

.page .msg-box .sticks .icon_bg {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  text-align: center;
}

.page .msg-box .sticks .icon_bg image {
  width: 46rpx;
  height: 46rpx;
  vertical-align: middle;
}

.page .msg-box .sticks .icon_bg.success_bg {
  background: #dd8f97;
}

.page .msg-box .sticks .icon_bg.status_bg {
  background: #d96471;
}

.page .msg-box .status {
  margin-left: 12rpx;
  padding-top: 36rpx;
}

.page .msg-box .status .pay_status {
  height: 90rpx;
  line-height: 90rpx;
  margin-top: 24rpx;
}

.page .msg-box .status .goods-status {
  height: 90rpx;
  margin-top: 110rpx;
}

.page .msg-box .status .goods-status .num {
  color: #737373;
}

.page .goods-loc {
  padding: 0 104rpx;
}

.page .goods-loc .box-gif image {
  width: 100%;
}

.page .wrap-bn {
  padding: 0 184rpx;
}

.page .wrap-bn .bn {
  height: 72rpx;
  border: 2rpx solid #404040;
  border-radius: 6rpx;
  line-height: 72rpx;
  text-align: center;
  font-size: 32rpx;
  color: #404040;
}

.page .wrap-bn .question {
  color: #737373;
  font-size: 24rpx;
  text-align: center;
  margin-top: 32rpx;
}
